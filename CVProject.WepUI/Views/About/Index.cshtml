@model CVProject.WepUI.Model.AboutModel.AboutListViewModel

<table class="table">
    <thead>
        <tr>
            <th>Firstname</th>
            <th>Lastname</th>
            <th>Address</th>
            <th>Phone</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        <tr>
        @foreach (var item in Model.Abouts)
        {
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Address</td>
                <td>@item.Phone</td>
                <td>@item.Description</td>
            }      
        </tr>
    </tbody>
</table>
